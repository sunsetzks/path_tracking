{
  "name": "hybrid-astar-ecal",
  "version": "1.0.0",
  "description": "Pure C++ Hybrid A* path planning with eCAL visualization",
  "keywords": [
    "path-planning",
    "hybrid-astar",
    "ecal",
    "foxglove",
    "robotics",
    "autonomous-driving"
  ],
  "author": "Path Planning Team",
  "license": "MIT",
  "repository": {
    "type": "git",
